class Solution {
    public int averageValue(int[] nums) {
        int count=0,sum=0;
        for(int i=0;i<nums.length;i++){
            int n=nums[i];
            if(n%2==0 && n%3==0){
                count++;
                sum+=n;

            }
        }
        return sum==0?0:sum/count;
    }
}
