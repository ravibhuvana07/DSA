class Solution {
    public int differenceOfSum(int[] nums) {
        int sum=0,digit=0;
        for(int i=0;i<nums.length;i++){
            int n=nums[i];
            sum+=nums[i];
            while(n>0){
                digit+=n%10;
                n/=10;
            }
        }
        return sum-digit;
    }
}
